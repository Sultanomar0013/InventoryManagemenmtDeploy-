{
    "version": 2,
    "name":"inventory-management",
    "builds":[
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes":[
        {
            "src": "/(.*)","dest":"/app.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": {
            "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}