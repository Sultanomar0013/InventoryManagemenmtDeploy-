{
    "version": 2,
    "name": "inventory-management",
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.js"
        },
        {
            "src": "/api/(.*)",
            "methods": [
                "OPTIONS"
            ],
            "status": 204
        
        }
    ]
}